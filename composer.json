{
    "name": "phucnguyenvn/laravel-force-https-middleware",
    "description": "Laravel module which force all request to https protocol",
    "keywords": ["laravel", "middleware", "https"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Phuc Nguyen",
            "email": "hong.phuc@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "PhucNguyenVn\\Http\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhucNguyenVn\\Http\\Middleware\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
